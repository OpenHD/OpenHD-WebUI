{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Documentation": {
    "LocalDocsRoot": "/usr/local/share/openhd/docs",
    "RequestPath": "/docs",
    "LocalIntroPage": "introduction/index.html"
  },
  "FilesFolders": [
    "/home/openhd/Videos/",
    "/Videos"
  ],
  "SystemCommands": [
    {
      "Id": "sys-res-opendh",
      "DisplayName": "Restart OpenHD",
      "Command": "systemctl restart openhd"
    },
    {
      "Id": "sys-res-qopendh",
      "DisplayName": "Restart qOpenHD",
      "Command": "systemctl restart qopenhd",
      "IsGroundOnly": "true"
    },
    {
      "Id": "sys-reboot",
      "DisplayName": "Reboot system",
      "Command": "systemctl reboot"
    },
    {
      "Id": "sys-off",
      "DisplayName": "Poweroff system",
      "Command": "systemctl poweroff"
    }
  ],
  "SystemFiles": [
    {
      "Id": "conf",
      "DisplayName": "config.txt",
      "Path": "/boot/config.txt"
    },
    {
      "Id": "version",
      "DisplayName": "openhd_version.txt",
      "Path": "/boot/openhd_version.txt"
    }
  ],
  "SettingsDirectories": [
    "/usr/local/share/openhd",
    "/usr/local/share/openhd/settings",
    "/boot/openhd",
    "/boot/openhd/settings"
  ],
  "UpdateConfig": {
    "UpdateFile": "/usr/local/share/openhd/update.zip"
  }
}
